import java.Util.Scanner;

/*
 * Codes for testing GitHub, at the same time practice java skills.
 */
public class CodeExample {
    private String name;
    private int age;
    private int id;
    
    /**
    * Class Constructor.
    * 
    * @Parameters name - person's name
    * @Parameters age - person's age
    * @Parameters id - person's id
    */
    public CodeExample(String name, int age, int id) {
        this.name = name;
        this.age = age;
        this.id = id;
        
    }

    public void main(String[] args) {
        CodeExample person = new CodeExample(Jayjay, 32, 11432494);
        Scanner input = new Scanner(System.in);
        System.out.println(" the boy " + person.name + " under the id " + person.id + " has " + person.age + " years.");
        input.close();
        
    }
    
}
